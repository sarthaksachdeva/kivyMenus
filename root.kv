<CustButton@Button>:
    background_normal: ''
    font_size: 12
    markup: True

<CustButton2@Button>:
    background_normal: ''
    font_size: 12
    markup: True
    background_color: (0.0, 0.0, 0.0, 0)

rootWidget:
    #:import C kivy.utils.get_color_from_hex
    boxes: _boxes
    navBar: _navBar

    #HOME SCREEN WIDGETS
    AnchorLayout:
        anchor_x: 'center'
        anchor_y: 'bottom'
        Carousel:
            id: _boxes
            size_hint: 1, 0.93

    #NAVBAR
    AnchorLayout:
        anchor_x: 'left'
        anchor_y: 'top'
        id: _navBar

